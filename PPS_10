marks = [float(input(f"subject {i +1}: ")) for i in range(5)]

average = round (sum(marks) / len(marks), 2)
if 90 <= average <= 100:
	grade = 'A'
elif 80 <= average < 90:
	grade ='B'
elif 70 <= average < 80:
	grade = 'C'
elif 60 <= average < 70:
	grade = 'D'
else:
	grade = 'F'


print(f"Average Marks: {average:.2f}")
print(f"Grade: {grade}")
